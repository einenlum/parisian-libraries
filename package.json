{
  "name": "parisian-libraries",
  "version": "0.1.0",
  "description": "A TypeScript library for querying the Paris public library system (bibliotheques.paris.fr)",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc -w",
    "test": "NODE_OPTIONS='--experimental-vm-modules' jest",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "paris",
    "library",
    "libraries",
    "bibliotheques",
    "books",
    "france"
  ],
  "author": "Yann Rabiller <contact@einenlum.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "jest": "^30.2.0",
    "ts-jest": "^29.4.6",
    "typescript": "^5.5.3"
  },
  "dependencies": {
    "date-fns": "^3.6.0",
    "node-html-parser": "^6.1.13"
  }
}
